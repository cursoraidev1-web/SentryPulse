{
    "name": "soothetech/sentrypulse",
    "type": "project",
    "description": "SentryPulse - Website Monitoring & Analytics Platform",
    "keywords": ["monitoring", "analytics", "uptime", "saas"],
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-redis": "*",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.8",
        "predis/predis": "^2.2",
        "vlucas/phpdotenv": "^5.6",
        "symfony/console": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mime": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan cache:clear"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
